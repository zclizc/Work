<view class="weui-cells weui-cells_after-title">
    <view aria-role="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="choseImage">
        <view aria-role="link" aria-labelledby="js_cell_tl1_bd js_cell_tl1_ft" id="js_cell_tl1_bd" class="weui-cell__bd">头像</view>
        <view class="weui-cell__ft weui-cell__ft_in-access" id="js_cell_tl1_ft" aria-hidden="true">
            <image class="head_img" src="{{userinfo.head_img?userinfo.head_img:'/images/default_head_circle.png'}}">
            </image>
        </view>
    </view>
    <navigator aria-role="" url="./change?changeWhat=name" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view aria-role="link" aria-labelledby="js_cell_tl2_bd js_cell_tl2_ft" id="js_cell_tl2_bd" class="weui-cell__bd">姓名</view>
        <view id="js_cell_tl2_ft" aria-hidden="true" class="weui-cell__ft weui-cell__ft_in-access">{{userinfo.name}}</view>
    </navigator>
    <navigator aria-role="" url="./change?changeWhat=tel" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view aria-role="link" aria-labelledby="js_cell_tl2_bd js_cell_tl2_ft" id="js_cell_tl2_bd" class="weui-cell__bd">手机号</view>
        <view id="js_cell_tl2_ft" aria-hidden="true" class="weui-cell__ft weui-cell__ft_in-access">{{userinfo.tel}}</view>
    </navigator>
    <navigator aria-role="" url="./change?changeWhat=sex" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view aria-role="link" aria-labelledby="js_cell_tl2_bd js_cell_tl2_ft" id="js_cell_tl2_bd" class="weui-cell__bd">性别</view>
        <view id="js_cell_tl2_ft" aria-hidden="true" class="weui-cell__ft weui-cell__ft_in-access">{{sex_array[userinfo.sex]}}</view>
    </navigator>
    <navigator aria-role="" url="./change?changeWhat=school" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view aria-role="link" aria-labelledby="js_cell_tl2_bd js_cell_tl2_ft" id="js_cell_tl2_bd" class="weui-cell__bd">学校</view>
        <view id="js_cell_tl2_ft" aria-hidden="true" class="weui-cell__ft weui-cell__ft_in-access">{{userinfo.school}}</view>
    </navigator>
    <navigator aria-role="" url="./change?changeWhat=number" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view aria-role="link" aria-labelledby="js_cell_tl2_bd js_cell_tl2_ft" id="js_cell_tl2_bd" class="weui-cell__bd">学号</view>
        <view id="js_cell_tl2_ft" aria-hidden="true" class="weui-cell__ft weui-cell__ft_in-access">{{userinfo.number}}</view>
    </navigator>
    <navigator aria-role="" url="./change?changeWhat=enter_year" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view aria-role="link" aria-labelledby="js_cell_tl2_bd js_cell_tl2_ft" id="js_cell_tl2_bd" class="weui-cell__bd">入学年份</view>
        <view id="js_cell_tl2_ft" aria-hidden="true" class="weui-cell__ft weui-cell__ft_in-access">{{userinfo.enter_year}}</view>
    </navigator>
</view>